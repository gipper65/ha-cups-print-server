{
  "name": "CUPS Print Server",
  "version": "1.1.3",
  "slug": "cups-print-server",
  "description": "CUPS printing system for Home Assistant",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "ports": {
    "631/tcp": 631,
    "5000/tcp": 5000
  },
  "ports_description": {
    "631/tcp": "CUPS Web Interface",
    "5000/tcp": "Print Service API"
  },
  "host_network": true,
  "devices": [
    "/dev/bus/usb"
  ],
  "map": [
    "config:rw",
    "share:rw"
  ],
  "privileged": [
    "SYS_ADMIN"
  ],
  "options": {
    "admin_password": "admin"
  },
  "schema": {
    "admin_password": "password"
  }
}
